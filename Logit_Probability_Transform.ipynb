   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "def L2P(l):\n",
    "    return np.exp(l) / (1 + np.exp(l))\n",
    "\n",
    "\n",
    "def P2L(p):\n",
    "    return np.log(p / (1 - p))\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    v = 0.8\n",
    "    l = P2L(v)\n",
    "    p = L2P(l)\n",
    "    print(f\"Value: {v}\")\n",
    "    print(f\"Logit: {l}\")\n",
    "    print(f\"Probability: {p}\")"
   ]
